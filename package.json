{
  "name": "adonis-types",
  "version": "1.0.13",
  "description": "Types for AdonisJS 4.1",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/golostos/adonis-types.git"
  },
  "scripts": {
    "gen": "tsc ../../node_modules/@adonisjs/ignitor/src/**/*.js --declaration --declarationMap --allowJs --emitDeclarationOnly --outDir Ignitor",
    "precommit": "npm version patch --no-git-tag-version && git add package.json",
    "postcommit": "npm publish"
  },
  "dependencies": {
    "tarn": "^3.0.1"
  },
  "author": "Alexander Dmitriev",
  "license": "MIT"
}
